{
  "expo": {
    "name": "test",
    "slug": "instalink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/InstaLink.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#191a1c"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },

      "eas": {
        "projectId": "c6f38267-fd15-4226-9ead-441ba35b1f00"
      }
    },
    "owner": "salman7355"
  }
}
